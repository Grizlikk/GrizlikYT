	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <FileAlignment>8192</FileAlignment>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="Photo icon.ico" />
	</ItemGroup>

</Project>